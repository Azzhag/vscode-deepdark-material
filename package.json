{
  "name": "deepdark-material",
  "displayName": "Deepdark Material Theme",
  "description": "Clean real dark material theme for Visual Studio Code Editor",
  "version": "1.9.0",
  "publisher": "Nimda",
  "engines": {
    "vscode": "*"
  },
  "__metadata": {
    "publisherDisplayName": "Nimda",
    "publisherId": "Nimda"
  },
  "homepage": "https://github.com/onurrozkaan/vscode-deepdark-material/blob/master/README.md",
  "license": "MIT",
  "categories": [
    "Themes"
  ],
  "icon": "img/logo.png",
  "keywords": [
    "deepdark",
    "darkgrey",
    "dark",
    "material",
    "black",
    "theme"
  ],
  "galleryBanner": {
    "color": "#f5f5f5",
    "theme": "light"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onurrozkaan/vscode-deepdark-material"
  },
  "contributes": {
    "themes": [
      {
        "label": "Deepdark Material",
        "uiTheme": "vs-dark",
        "path": "./themes/deepdark-material.json"
      }
    ]
  },
  "scripts": {
    "format:fix": "eslint --fix '*.{js,json}' 'themes/**/*.{js,json}'",
    "lint": "npm run lint:js && npm run lint:json && npm run lint:md",
    "lint:js": "eslint '*.js' 'themes/**/*.js'",
    "lint:json": "eslint '*.json' 'themes/**/*.json'",
    "lint:md": "remark '*.md'"
  },
  "devDependencies": {
    "babel-eslint": "8.2.5",
    "eslint": "4.19.1",
    "eslint-config-arcticicestudio-base": "0.3.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-prettier": "2.6.1",
    "prettier": "1.13.7",
    "remark-cli": "5.0.0",
    "remark-preset-lint-arcticicestudio": "0.1.0"
  }
}
